CREATE TABLE `instagramstats`.`ig_report` ( `date` DATE NOT NULL , `account` VARCHAR(35) NOT NULL , `userid` VARCHAR(35) NOT NULL , `followers` LONGTEXT NOT NULL , `followees` LONGTEXT NOT NULL , `count_followers` INT NOT NULL , `count_followees` INT NOT NULL , `total_likes` INT NOT NULL , `total_comments` INT NOT NULL , `total_post` INT NOT NULL , `engagement` DECIMAL(5,2) NOT NULL ) ENGINE = InnoDB; 